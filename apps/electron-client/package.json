{
  "name": "electron-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "main.js",
  "scripts": {
    "renderer:dev": "npx vite --config src/vite.config.ts",
    "electron:dev": "npx wait-on http://localhost:5173 && electron .",
    "dev": "npx electron-rebuild && npx cross-env NODE_ENV=development npx concurrently \"npm:renderer:dev\" \"npm:electron:dev\"",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:user": "electron . --test",
    "build:addon": "cd addon && node-gyp rebuild --target=29.4.6 --arch=x64 --dist-url=https://electronjs.org/headers",
    "build:renderer": "tsc -p src/tsconfig.app.json && vite build --config src/vite.config.ts",
    "build": "npm run build:renderer && npm run build:addon",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/react": "^0.27.8",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.7",
    "axios": "^1.9.0",
    "chardet": "^2.1.0",
    "electron": "^29.4.6",
    "electron-store": "^7.0.3",
    "emailjs-mime-parser": "^2.0.7",
    "googleapis": "^148.0.0",
    "iconv-lite": "^0.6.3",
    "lucide-react": "^0.511.0",
    "mailparser": "^3.7.2",
    "mammoth": "^1.9.0",
    "node-addon-api": "^8.3.1",
    "node-gyp": "^11.2.0",
    "node-imap": "^0.9.6",
    "nodemailer": "^6.10.1",
    "pretendard": "^1.3.9",
    "quill": "^2.0.3",
    "quoted-printable": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-quill": "^2.0.0",
    "react-quilljs": "^2.0.5",
    "react-router-dom": "^7.5.2",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.21",
    "electron-rebuild": "^3.2.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "msw": "^2.7.5",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "wait-on": "^8.0.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
